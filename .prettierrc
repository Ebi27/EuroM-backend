{
  "semi": false,
  "singleQuote": true,
  "useTabs": true,
  "tabWidth": 2,
 "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "printWidth": 100,
  "proseWrap": "always",
  "bracketSpacing": false,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ]
}
